{
  "name": "ilevus",
  "version": "1.0.0",
  "description": "ilevus",
  "scripts": {
    "whichnode":  "where webpack",
    "prestart": "npm install",
    "devbuild": "webpack --colors",
    "devstart": "set NODE_ENV=development && webpack-dev-server --compress --progress --port 8081 --inline -w",
    "devwatch": "set NODE_ENV=development && webpack -d --watch --watch-poll --colors --progress",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "set NODE_ENV=production && webpack --colors --progress --config webpack.production.config.js --optimize-dedupe --optimize-occurrence-order --optimize-minimize"
  },
  "author": "Renato R. R. de Oliveira",
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.22.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.6.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "blueimp-file-upload": "^9.12.4",
    "flux": "^2.1.1",
    "history": "^2.1.1",
    "jquery": "^2.2.3",
    "moment": "^2.13.0",
    "numeral": "^1.5.3",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-maskedinput": "^3.1.3",
    "react-router": "^2.4.0",
    "string": "^3.3.1",
    "tether": "^1.3.2",
    "toastr": "^2.1.2",
    "underscore": "^1.8.3"
  },
  "license": "UNLICENSED",
  "-vs-binding": {
    "ProjectOpened": [
      "devwatch"
    ]
  }
}
