/* eslint-disable no-underscore-dangle */
// https://github.com/diegohaz/arc/wiki/Example-app
import 'react-hot-loader/patch';
import 'babel-polyfill';
import React from 'react';
import { render } from 'react-dom';
import { Provider } from 'react-redux';
import { BrowserRouter } from 'react-router-dom';
import { ServerStateProvider } from 'react-router-server';

import { basename } from 'config';
import configureStore from 'store/configure';
import api from 'services/api';
import App from 'components/App';

var serverState = window.__SERVER_STATE__;
var initialState = window.__INITIAL_STATE__;
var store = configureStore(initialState, { api: api.create() });

var renderApp = function renderApp() {
  return React.createElement(
    ServerStateProvider,
    { state: serverState },
    React.createElement(
      Provider,
      { store: store },
      React.createElement(
        BrowserRouter,
        { basename: basename },
        React.createElement(App, null)
      )
    )
  );
};

var root = document.getElementById('app');
render(renderApp(), root);

if (module.hot) {
  module.hot.accept('components/App', function () {
    require('components/App');
    render(renderApp(), root);
  });
}
;

var _temp = function () {
  if (typeof __REACT_HOT_LOADER__ === 'undefined') {
    return;
  }

  __REACT_HOT_LOADER__.register(serverState, 'serverState', 'C:/Projetos/Nova pasta/ilevus/Ilevus/Views/src/client.js');

  __REACT_HOT_LOADER__.register(initialState, 'initialState', 'C:/Projetos/Nova pasta/ilevus/Ilevus/Views/src/client.js');

  __REACT_HOT_LOADER__.register(store, 'store', 'C:/Projetos/Nova pasta/ilevus/Ilevus/Views/src/client.js');

  __REACT_HOT_LOADER__.register(renderApp, 'renderApp', 'C:/Projetos/Nova pasta/ilevus/Ilevus/Views/src/client.js');

  __REACT_HOT_LOADER__.register(root, 'root', 'C:/Projetos/Nova pasta/ilevus/Ilevus/Views/src/client.js');
}();

;