                                ) : ""
                            )
                        ),
                        React.createElement(
                            "li",
                            { className: "ilv-media ilv-media-middle pb-0" },
                            React.createElement(
                                "div",
                                { className: "ilv-media-body" },
                                React.createElement(
                                    "strong",
                                    { className: "ilv-text-small ilv-text-uppercase text-muted" },
                                    Messages.get("LabelStep"),
                                    " 5",
                                    this.state.professionalData.ServicesInfo ? React.createElement(
                                        "i",
                                        { className: "material-icons md-18 text-success", style: { margin: "-3px 0 0 5px" } },
                                        "\uE86C"
                                    ) : ""
                                ),
                                React.createElement(
                                    "h3",
                                    { className: "m-a-0" },
                                    Messages.get("TextOfferedServices")
                                ),
                                React.createElement(
                                    "p",
                                    null,
                                    Messages.get("TextServicesProfessionalInfo")
                                ),
                                this.state.professionalData.BasicInfo && this.state.professionalData.AddressInfo && this.state.professionalData.EducationInfo && this.state.professionalData.CareerInfo ? this.state.professionalData.ServicesInfo ? React.createElement(
                                    Link,
                                    { className: "ilv-font-weight-semibold", to: "/become-a-professional/services" },
                                    Messages.get("LabelChangeInformation")
                                ) : React.createElement(
                                    Link,
                                    { className: "ilv-btn ilv-btn-success", to: "/become-a-professional/services" },
                                    Messages.get("LabelContinue")
                                ) : ""
                            )
                        ),
                        React.createElement(
                            "li",
                            { className: "ilv-media ilv-media-middle pb-0" },
                            React.createElement(
                                "div",
                                { className: "ilv-media-body" },
                                React.createElement(
                                    "strong",
                                    { className: "ilv-text-small ilv-text-uppercase text-muted" },
                                    Messages.get("LabelStep"),
                                    " 6",
                                    this.state.professionalData.ServicesInfo ? React.createElement(
                                        "i",
                                        { className: "material-icons md-18 text-success", style: { margin: "-3px 0 0 5px" } },
                                        "\uE86C"
                                    ) : ""
                                ),
                                React.createElement(
                                    "h3",
                                    { className: "m-a-0" },
                                    Messages.get("TextTitleBankAccountStep")
                                ),
                                React.createElement(
                                    "p",
                                    null,
                                    Messages.get("TextServicesProfessionalBankAccount")
                                ),
                                this.state.professionalData.BasicInfo && this.state.professionalData.AddressInfo && this.state.professionalData.EducationInfo && this.state.professionalData.CareerInfo ? this.state.professionalData.ServicesInfo ? React.createElement(
                                    Link,
                                    { className: "ilv-font-weight-semibold", to: "/become-a-professional/bankAccount" },
                                    Messages.get("LabelChangeInformation")
                                ) : React.createElement(
