    if (typeof __REACT_HOT_LOADER__ === 'undefined') {
        return;
    }

    __REACT_HOT_LOADER__.register(format, "format", "C:/Projetos/Nova pasta/ilevus/Ilevus/Views/legacy/jsx/core/util/CurrencyUtils.jsx");
}();

