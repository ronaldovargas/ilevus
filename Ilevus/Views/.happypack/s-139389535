import React from 'react';
import { connect } from 'react-redux';
import { fromSocial } from 'store/selectors';
import { modalShow, socialLogout } from 'store/actions';

import { UserButton } from 'components';

var UserButtonContainer = function UserButtonContainer(props) {
  return React.createElement(UserButton, props);
};

var mapStateToProps = function mapStateToProps(state) {
  return {
    user: fromSocial.getUser(state)
  };
};

var mapDispatchToProps = function mapDispatchToProps(dispatch) {
  return {
    onLogin: function onLogin() {
      return dispatch(modalShow('login'));
    },
    onLogout: function onLogout() {
      return dispatch(socialLogout());
    }
  };
};

var _default = connect(mapStateToProps, mapDispatchToProps)(UserButtonContainer);

export default _default;
;

