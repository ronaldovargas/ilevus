// https://github.com/diegohaz/arc/wiki/Actions
// https://github.com/diegohaz/arc/wiki/Example-redux-modules#modal
export var MODAL_SHOW = 'MODAL_SHOW';
export var MODAL_HIDE = 'MODAL_HIDE';

export var modalShow = function modalShow(name) {
  return {
    type: MODAL_SHOW,
    payload: {
      name: name
    },
    meta: {
      gtm: name
    }
  };
};

export var modalHide = function modalHide(name) {
  return {
    type: MODAL_HIDE,
    payload: {
      name: name
    }
  };
};
;

var _temp = function () {
  if (typeof __REACT_HOT_LOADER__ === 'undefined') {
    return;
  }

  __REACT_HOT_LOADER__.register(MODAL_SHOW, 'MODAL_SHOW', 'C:/Projetos/Nova pasta/ilevus/Ilevus/Views/src/store/modal/actions.js');

  __REACT_HOT_LOADER__.register(MODAL_HIDE, 'MODAL_HIDE', 'C:/Projetos/Nova pasta/ilevus/Ilevus/Views/src/store/modal/actions.js');

  __REACT_HOT_LOADER__.register(modalShow, 'modalShow', 'C:/Projetos/Nova pasta/ilevus/Ilevus/Views/src/store/modal/actions.js');

  __REACT_HOT_LOADER__.register(modalHide, 'modalHide', 'C:/Projetos/Nova pasta/ilevus/Ilevus/Views/src/store/modal/actions.js');
}();

;