import React from 'react';
import PropTypes from 'prop-types';
import { connect } from 'react-redux';
import { fromAsync } from 'store/selectors';

var GenericContainer = function GenericContainer(_ref) {
  var loading = _ref.loading;
  return React.createElement('div', { className: loading ? 'loading' : '' });
};

GenericContainer.propTypes = {
  loading: PropTypes.bool
};

var mapStateToProps = function mapStateToProps(state) {
  return {
    loading: fromAsync.isPending(state)
  };
};

var _default = connect(mapStateToProps)(GenericContainer);

export default _default;
