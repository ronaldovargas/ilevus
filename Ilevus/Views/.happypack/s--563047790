import React from 'react';
import { reduxForm } from 'redux-form';
import { resourceCreateRequest } from 'store/actions';
import { createValidator, required } from 'services/validation';

import { PostForm } from 'components';

var PostFormContainer = function PostFormContainer(props) {
  return React.createElement(PostForm, props);
};

var onSubmit = function onSubmit(data, dispatch) {
  return dispatch(resourceCreateRequest('posts', data));
};

var validate = createValidator({
  title: [required],
  body: [required]
});

var _default = reduxForm({
  form: 'PostForm',
  destroyOnUnmount: false,
  onSubmit: onSubmit,
  validate: validate
})(PostFormContainer);

export default _default;
;

var _temp = function () {
  if (typeof __REACT_HOT_LOADER__ === 'undefined') {
    return;
  }

  __REACT_HOT_LOADER__.register(PostFormContainer, 'PostFormContainer', 'C:/Projetos/Nova pasta/ilevus/Ilevus/Views/src/containers/PostForm.js');

  __REACT_HOT_LOADER__.register(onSubmit, 'onSubmit', 'C:/Projetos/Nova pasta/ilevus/Ilevus/Views/src/containers/PostForm.js');

  __REACT_HOT_LOADER__.register(validate, 'validate', 'C:/Projetos/Nova pasta/ilevus/Ilevus/Views/src/containers/PostForm.js');

  __REACT_HOT_LOADER__.register(_default, 'default', 'C:/Projetos/Nova pasta/ilevus/Ilevus/Views/src/containers/PostForm.js');
}();

